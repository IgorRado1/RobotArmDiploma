    "event": "btnReset.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": false,
        "sourceLines": [
          "let number = parseInt(TcHmi.Controls.get('textStevilko').getText(), 10);",
          "",
          "if (number >= 1 && number <= 4) {",
          "    let binId = 'txtbBin' + number;",
          "    let cbId = 'cbBin' + number;",
          "",
          "    let control = TcHmi.Controls.get(binId);",
          "    let checkbox = TcHmi.Controls.get(cbId);",
          "",
          "    if (control) {",
          "        control.setText('0');",
          "    }",
          "",
          "    if (checkbox) {",
          "        checkbox.setToggleState('Normal');",
          "    }",
          "} else {",
          "    console.warn('Vpiši število med 1 in 4!');",
          "}"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
