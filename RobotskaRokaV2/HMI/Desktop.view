          "        value = 4; // Bagel",
          "    }",
          "",
          "    TcHmi.Controls.get('textStevilko').setText(String(value));",
          "}"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
