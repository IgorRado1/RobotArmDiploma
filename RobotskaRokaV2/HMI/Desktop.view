        "sourceLines": [
          "let color = TcHmi.Controls.get('txtbColor').getText().trim().toLowerCase();",
          "let weight = TcHmi.Controls.get('txtbWeight').getText().trim().toLowerCase();",
          "",
          "let value = 0;",
          "",
          "if (color === '' || weight === '') {",
          "    // Ni še oboje izpolnjeno, ne nastavljaj nič",
