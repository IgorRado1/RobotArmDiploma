<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "TcHmiButton.onPressed",
    "actions": []
  },
  {
    "event": "btnAdBread.onPressed",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "let value = parseInt(TcHmi.Controls.get('textStevilko').getText(), 10);",
          "let targetId = '';",
          "",
          "if (value === 1) {",
          "    targetId = 'txtbBin1';",
          "} else if (value === 2) {",
          "    targetId = 'txtbBin2';",
          "} else if (value === 3) {",
          "    targetId = 'txtbBin3';",
          "} else if (value === 4) {",
          "    targetId = 'txtbBin4';",
          "} else {",
          "    console.warn('Neveljavna vrednost. Vpiši število 1–4.');",
          "}",
          "",
          "let control = TcHmi.Controls.get(targetId);",
          "",
          "if (control) {",
          "    let current = parseInt(control.getText(), 10) || 0;",
          "    current += 1;",
          "    control.setText(String(current));",
          "} else {",
          "    console.error('Kontrola ni najdena:', targetId);",
          "}",
          ""
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]
</script>
