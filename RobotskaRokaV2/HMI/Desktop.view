          "var path = \"\";",
          "",
          "switch (value) {",
          "    case 1: path = \"Images/Bread/toast.png\"; break;",
          "    case 2: path = \"Images/Bread/baguette.jpeg\"; break;",
          "    case 3: path = \"Images/Bread/rye.png\"; break;",
          "    case 4: path = \"Images/Bread/bagel.png\"; break;",
          "    default: path = \"Images/Bread/empty.png\";",
          "}",
          "",
          "TcHmi.Symbol.writeEx('%i%currentImagePath%/i%', path, function(data) {",
          "    if (data.error !== TcHmi.Errors.NONE) {",
          "        console.error(\"Napaka pri nastavitvi slike:\", data.error);",
          "    }",
          "});",
          "",
          "",
          "",
          "/*",
          "//S SENSORJI",
          "var id = TcHmi.Symbol.readEx('%i%GVL.breadTypeImageID%/i%');",
          "var path = '';",
          "",
          "switch (id) {",
          "    case 1: path = 'Images/Bread/toast.png'; break;",
